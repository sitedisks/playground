<!--pages/map/map.wxml-->
<view>
<map
  id="map"
  longitude="{{lng}}"
  latitude="{{lat}}"
  scale="14"
  controls="{{controls}}"
  bindcontroltap="controltap"
  markers="{{markers}}"
  bindmarkertap="markertap"
  polyline="{{polyline}}"
  bindregionchange="regionchange"
  show-location
  style="width: 100%; height: 600px;"
></map>
</view>
